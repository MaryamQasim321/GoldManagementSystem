<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.goldmanagemensystem.ReportsDisplayController" stylesheets="@Reports.css">
   <children>
      <TableView   AnchorPane.leftAnchor="50" AnchorPane.rightAnchor="50"
