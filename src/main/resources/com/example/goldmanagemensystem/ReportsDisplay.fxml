        <columns>
          <TableColumn styleClass="table-cell" fx:id="dateColumn" prefWidth="300.0" text="Date" />
          <TableColumn styleClass="table-cell" fx:id="purchaseColumn" prefWidth="300.0" text="Purchase" />
            <TableColumn styleClass="table-cell" fx:id="salesColumn" prefWidth="300.0" text="Sale" />
            <TableColumn styleClass="table-cell" fx:id="expensesColumn" prefWidth="300.0" text="Expenses" />
        </columns>
      </TableView>
       <Button mnemonicParsing="false" text="Back to main screen" onAction="#onClickBackButton"/>

   </children>
</VBox>
